{
  "name": "adcbankaapp",
  "version": "1.0.0",
  "description": "andela developer challenge bootcamp app",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./api/index.js",
    "test": "set TEST_ENV=test && nyc --reporter=html --reporter=text mocha --require @babel/register ./api/test/*.js --exit",
    "clean": "rimraf build && mkdir build",
    "build-babel": "npm run clean && ./node_modules/.bin/babel ./api -d ./build/ --ignore ./api/coverage",
    "build": "npm run build-babel",
    "start": "node ./build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desiba/ADCBanka.git"
  },
  "keywords": [
    "banka",
    "ADC",
    "Andela",
    "nodejs"
  ],
  "author": "desmond",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/desiba/ADCBanka/issues"
  },
  "homepage": "https://github.com/desiba/ADCBanka#readme",
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^3.0.5",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "node-uuid": "^1.4.8",
    "nyc": "^13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/parser": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
